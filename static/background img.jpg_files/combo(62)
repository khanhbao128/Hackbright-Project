YUI.add("hermes-template-header-banner",function(e,a){var r=e.Template.Handlebars.revive({compiler:[7,">= 4.0.0"],main:function(e,a,r,n,t){return'<div class="header-banner">\n</div>\n'},useData:!0}),n={};e.Array.each([],function(a){var r=e.Template.get("hermes/"+a);r&&(n[a]=r)}),e.Template.register("hermes/header-banner",function(a,t){return t=t||{},t.partials=t.partials?e.merge(n,t.partials):n,r(a,t)})},"@VERSION@",{requires:["template-base","handlebars-base"]});YUI.add("header-banner-view",function(e,i){require("hermes-core/flog")(i);e.FlickrView.create(this.name,e.FlickrView,[],{langBundles:this.details.langBundles,initializer:function(e){this.params=e,this.subview=this.params.subview},loadState:function(){return e.Promise.resolve()},buildContainer:function(){this.setContainerWithTemplate("header-banner")},activate:function(){var e=this,i=this.get("container");this.subviewParent=i.one(".header-banner"),this.subview&&(this.setupSubview(),this.subview.on("dismiss",function(i){e.dismiss()}))},setupSubview:function(){var i=this;this.subview.initialize().then(function(){var e=i.subview.get("container");if(i.subview.canShowBanner())return i.subviewParent.empty(),i.subviewParent.appendChild(e),i.subview.activate();i.dismiss()}).then(function(){e.fire("headerBanner:activated")})},dismiss:function(){this.get("container").remove(),e.fire("headerBanner:dismissed"),this.destroy()}})},"@VERSION@",{requires:["flickr-view","hermes-template-header-banner"],langBundles:["common"]});